{
  "name": "journey_plan",
  "version": "0.1.0",
  "private": true,
  "author": "Jérôme Procureur",
  "description": "MoneyTor Electron app",
  "license": "GG",
  "homepage": "./",
  "main": "build/electron/main.js",
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/lab": "^5.0.0-alpha.133",
    "@mui/material": "^5.11.15",
    "@mui/x-date-pickers": "^6.9.1",
    "@react-pdf-viewer/core": "^3.12.0",
    "@react-pdf/renderer": "^3.1.14",
    "@reduxjs/toolkit": "^1.9.3",
    "better-sqlite3": "^8.4.0",
    "dayjs": "^1.11.7",
    "dotenv": "^16.3.1",
    "electron-log": "^5.1.2",
    "leaflet": "^1.9.4",
    "primeicons": "^6.0.1",
    "primereact": "^9.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.5",
    "react-icons": "^4.8.0",
    "react-leaflet": "^4.2.1",
    "react-pdf": "^7.5.1",
    "react-places-autocomplete": "^7.3.0",
    "react-redux": "^8.0.5",
    "react-resize-detector": "^9.1.0",
    "react-router-dom": "^6.9.0",
    "react-scripts": "5.0.1",
    "react-transition-group": "^4.4.5"
  },
  "scripts": {
    "start-forge": "concurrently \"npm run start\" \"wait-on http://localhost:3000 && electron-forge start\"",
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build && tsc -p electron",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"pnpm run start\" \"tsc -p electron -w\" \"wait-on http://localhost:3000 && wait-on build/electron/main.js && electronmon .\"",
    "electron:build": "pnpm run build && tsc -p electron && electron-builder",
    "electron:start": "tsc -p electron && electronmon .",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-private-methods": "^7.24.1",
    "@babel/plugin-transform-private-property-in-object": "^7.24.1",
    "@babel/preset-react": "^7.24.1",
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "@types/better-sqlite3": "^7.6.4",
    "@types/leaflet": "^1.9.4",
    "@types/leaflet-providers": "^1.2.2",
    "@types/node": "^16.18.21",
    "@types/react": "^18.0.29",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-dom": "^18.0.11",
    "@types/react-places-autocomplete": "^7.2.9",
    "babel-loader": "^9.1.3",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^23.2.0",
    "electron-devtools-installer": "^3.2.0",
    "electronmon": "^2.0.2",
    "source-map-explorer": "^2.5.3",
    "wait-on": "^7.0.1"
  },
  "electronmon": {
    "patterns": [
      "!journey_plan.db"
    ]
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "asar": true,
        "extraResource": [
          "./journey_plan.db"
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "journyer_plan_name_aaa"
          }
        }
      ]
    }
  }
}
